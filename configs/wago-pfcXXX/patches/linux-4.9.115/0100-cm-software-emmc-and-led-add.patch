 #include "wago-pfc100-uart1.dtsi"
 #include "wago-pfc200v2-netx.dtsi"
 #include "wago-pfc200v2-dcan.dtsi"
@@ -59,3 +59,40 @@
 	/* disable oms driver for cmtest gpio test */
 	status = "disabled";
 };
+
+&u6_g {
+        label = "bf-green";
+        gpios = <&gpio3 2 0>;
+        linux,default-trigger = "none";
+};
+
+&u6_r {
+        label = "bf-red";
+        gpios = <&gpio3 3 0>;
+        linux,default-trigger = "none";
+};
+
+&u5_g {
+        label = "dia-green";
+        gpios = <&gpio3 6 0>;
+        linux,default-trigger = "none";
+};
+
+&u5_r {
+        label = "dia-red";
+        gpios = <&gpio3 7 0>;
+        linux,default-trigger = "none";
+};
+
+
+&u7_g {
+        label = "can-green";
+        gpios = <&gpio3 24 0>;
+        linux,default-trigger = "none";
+};
+
+&u7_r {
+        label = "can-red";
+        gpios = <&gpio3 25 0>;
+        linux,default-trigger = "none";
+};
